void print_hello(int n) {
    char arr[5];
    arr[0] = 'H';
    arr[1] = 'e';
    arr[2] = 'l';
    arr[3] = 'l';
    arr[4] = 'o';

    for (int i = 0; i < n; i = i + 1) {
        for (int j = 0; j < 5; j = j + 1) {
            print_char(arr[j]);
        }
         print_char('\n');
    }
}

int main() {
    int n = read_int();
    print_hello(n);
    return 0;
}
